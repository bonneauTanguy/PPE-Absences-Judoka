DROP TABLE IF EXISTS ELEVE;
DROP TABLE IF EXISTS CATEGORIE;
DROP TABLE IF EXISTS CEINTURE;
DROP TABLE IF EXISTS COURS;
DROP TABLE IF EXISTS POSSEDER;
DROP TABLE IF EXISTS TELEPHONE;
DROP TABLE IF EXISTS PARTICIPER;
CREATE CATEGORIE (IDCATEGORIE INTEGER PRIMARY KEY AUTOINCREMENT, LIBELLECATEGORIE VARCHAR(100));
CREATE CEINTURE (IDCEINTURE INTEGER PRIMARY KEY AUTOINCREMENT, LIBELLECEINTURE VARCHAR(100));
CREATE TELEPHONE(IDTELEPHONE INTEGER PRIMARY KEY AUTOINCREMENT, NUMEROTELEPHONE VARCHAR(10));
CREATE COURS (IDCOURS INTEGER PRIMARY KEY AUTOINCREMENT, DATECOURS DATE, HEUREDEBUTCOURS INTEGER, HEUREFINCOURS INTEGER);
CREATE ELEVE (IDELEVE INTEGER PRIMARY KEY AUTOINCREMENT, NOMELEVE VARCHAR(100), PRENOMELEVE VARCHAR(100), DATENAISSANCEELEVE DATE, FOREIGN KEY(IDCATEGORIEELEVE) REFERENCES CATEGORIE(IDCATEGORIE), FOREIGN KEY(IDCEINTUREELEVE) REFERENCES CEINTURE(IDCEINTURE));
CREATE POSSEDER(PRIMARY KEY(IDELEVE, IDTELEPHONE), FOREIGN KEY(IDELEVE) REFERENCES ELEVE(IDELEVE), FOREIGN KEY(IDTELEPHONE) REFERENCES TELEPHONE(IDTELEPHONE));
CREATE PARTICIPER(PRIMARY KEY(IDELEVE, IDCOURS), FOREIGN KEY(IDELEVE) REFERENCES ELEVE(IDELEVE));
