DROP TABLE IF EXISTS ELEVE;
DROP TABLE IF EXISTS CATEGORIE;
DROP TABLE IF EXISTS CEINTURE;
DROP TABLE IF EXISTS COURS;
DROP TABLE IF EXISTS POSSEDER;
DROP TABLE IF EXISTS TELEPHONE;
DROP TABLE IF EXISTS PARTICIPER;
CREATE TABLE CATEGORIE (IDCATEGORIE INTEGER PRIMARY KEY AUTOINCREMENT, LIBELLECATEGORIE VARCHAR(100));
CREATE TABLE CEINTURE (IDCEINTURE INTEGER PRIMARY KEY AUTOINCREMENT, LIBELLECEINTURE VARCHAR(100));
CREATE TABLE TELEPHONE(IDTELEPHONE INTEGER PRIMARY KEY AUTOINCREMENT, NUMEROTELEPHONE VARCHAR(10));
CREATE TABLE COURS (IDCOURS INTEGER PRIMARY KEY AUTOINCREMENT, DATECOURS DATE, HEUREDEBUTCOURS INTEGER, HEUREFINCOURS INTEGER);
CREATE TABLE ELEVE (IDELEVE INTEGER PRIMARY KEY AUTOINCREMENT, NOMELEVE VARCHAR(100), PRENOMELEVE VARCHAR(100), DATENAISSANCEELEVE DATE,IDCATEGORIEELEVE INT, IDCEINTUREELEVE INT , constraint fk_categ FOREIGN KEY(IDCATEGORIEELEVE) REFERENCES CATEGORIE(IDCATEGORIE), constraint fk_ceint foreign key(IDCEINTUREELEVE) references CEINTURE(IDCEINTURE));INSERT INTO ELEVE(NOMELEVE, PRENOMELEVE, DATENAISSANCEELEVE) Values("Dupont", "Pont", "12/11/2001");
